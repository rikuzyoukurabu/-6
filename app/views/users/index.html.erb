<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
<body>
<main>
  
<%= render 'books/book',books:@books,user:@user,book:@book %>
　<!--User一覧画面-->
　<h2>Users</h2>
　 <table class = "table" >
 　　　　 <thead>
   　    <th>image</th>
    　　<th>name</th>
       </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td>
       <%= attachment_image_tag user, :profile_image, :fill, 60, 60, format: 'jpeg', fallback: "no_image.jpg" %>
      </td>
      <td>
       <%= user.name %>
      </td>
      <td>
        <%= link_to "Show",user_path(user) %>
      </td>
      <% end %>
    </tr>
    </tbody>
   </table>
  </div>
 </div>
</div>
</body>